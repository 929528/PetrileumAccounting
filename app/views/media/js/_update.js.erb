<%- if item.errors.any? %>
$(".media-window[timestamp=<%= @timestamp%>] > .form-errors:first").html("<%=j render "media/error_messages", name: item %>").autohide(1000);
<%-else %>
$(".media-window[timestamp=<%= @timestamp%>]").media('hide');
$("#flash_notice").html("<%=j render '/flash_notice' %>").autohide(1000);
<%-end%>