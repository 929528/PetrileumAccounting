.tabbable.tabs-left class="tabs_#{items.model.model_name.element}"
	ul.nav.nav-tabs
		== render partial: 'nested_tabs/header', collection: items, as: :item
		li 
			a.text-center onclick='$(this).mw("add_tab")' data-path="#{add_path}"
				i.icon-plus
	.tab-content
		== render partial: 'nested_tabs/body', collection: items, as: :item